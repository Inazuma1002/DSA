#include<stdio.h>
#include<stdlib.h>
void mergesort(int* arr,int l,int r);
void merge(int* arr,int l,int mid,int r);
int main()
{
    int i=0,arr[20];
    int n;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    mergesort(arr,0,n-1);
    int j=0;
    for(j=0;j<n;j++)
    {
        printf("%d",arr[j]);
    }
    return 0;
}

void mergesort(int* arr,int l,int r)
{
    if(l<r)
    {
        int mid=(l+r)/2;
        mergesort(arr,l,mid);
        mergesort(arr,mid+ 1,r);
        merge(arr,l,mid,r);
    }
}
void merge(int*arr,int l,int mid,int r)
{
    int n1=mid-l+1;
    int n2=r-mid;
    int L[n1],R[n2];
    int i=0,j=0;
    for(i=0;i<n1;i++)
    {
        L[i]=arr[l+i];
    }
    for(j=0;j<n2;j++)
    {
        R[j]=arr[mid+1+j];
    }
    i=0;
    j=0;
    int k=l;
    while(i<n1 && j<n2)
    {
        if(L[i]<R[j])
        {
            arr[k]=L[i];
            i++;
        }
        else
        {
            arr[k]=R[j];       
            j++;
        }
        k++;
    }
    while(i<n1)
    {
        arr[k++]=L[i++];
    }
    while(j<n2)
    {
        arr[k++]=R[j++];
    }
}
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
